<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="assetsstate" extends="struts-default" namespace="/assetsstate">
		<global-results>
			<result name="list" type="redirectAction">list</result>
		</global-results>
		<action name="list" class="AssetsStateAction" method="list">
			<result name="success" type="dispatcher">
				/systemmanage/assetsstate/list.jsp
			</result>
		</action>
		<action name="add" class="AssetsStateAction" method="add">
		    <result name="success" type="dispatcher">
				/systemmanage/assetsstate/add.jsp
			</result>
		</action>
		<action name="del" class="AssetsStateAction" method="del">
		    <result name="success" type="redirect">
				list.action?message=${message}
			</result>
		</action>
		<action name="show" class="AssetsStateAction" method="show">
		    <result name="success" type="dispatcher">
				/systemmanage/assetsstate/update.jsp
			</result>
		</action>
		<action name="update" class="AssetsStateAction" method="update">
		    <result name="success" type="dispatcher">
				/systemmanage/assetsstate/update.jsp
			</result>
		</action>
		
	</package>
</struts>
