<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.combanc.itsm.pojo">

<class name="SysConfig" table="system_config">
	<id name="id" type="long" column="id">
		<generator class="assigned"/>
	</id>
	<property name="enField" type="string" column="enfield"/>
	<property name="chField" type="string" column="chfield"/>
	<property name="value" type="string" column="value"/>
	
	<property name="property1" type="string" column="property1"/>
	<property name="value1" type="string" column="value1"/>
	<property name="property2" type="string" column="property2"/>
	<property name="value2" type="string" column="value2"/>
	
	<property name="property3" type="string" column="property3"/>
	<property name="value3" type="string" column="value3"/>
	<property name="property4" type="string" column="property4"/>
	<property name="value4" type="string" column="value4"/>
	<many-to-one name="createUser" class="com.combanc.itsm.pojo.Users" column="createuser"></many-to-one>
	<property name="createTime" type="timestamp" column="createtime"></property>
	<many-to-one name="lastModifyUser" class="com.combanc.itsm.pojo.Users" column="lastmodifyuser"></many-to-one>
	<property name="lastModifyTime" type="timestamp" column="lastmodifytime"></property>
</class>

</hibernate-mapping>
