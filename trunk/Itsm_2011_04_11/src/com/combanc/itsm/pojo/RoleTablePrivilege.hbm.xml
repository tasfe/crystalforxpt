<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.combanc.itsm.pojo.RoleTablePrivilege" table="role_table_privilege" >
    <id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native" />
	</id>
		<property name="roleId" type="java.lang.Integer" column="roleID"/>
		<property name="tableId" type="java.lang.Integer" column="tableID"/>
        <property name="prop" type="java.lang.String">
            <column name="prop" />
        </property>
        <property name="privilege" type="java.lang.Integer">
            <column name="privilege" />
        </property>
    </class>
</hibernate-mapping>
