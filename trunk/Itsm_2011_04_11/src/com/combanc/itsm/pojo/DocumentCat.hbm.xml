<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.combanc.itsm.pojo">
    <class name="DocumentCat" table="document_cat" catalog="itsm">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native"></generator>
        </id>
        <property name="pid" type="java.lang.Integer">
            <column name="pid">
                <comment>父类id</comment>
            </column>
        </property>
        <property name="document" type="java.lang.String">
            <column name="document" length="100">
                <comment>目录英文名称</comment>
            </column>
        </property>
        <property name="documentSC" type="java.lang.String">
            <column name="document_SC" length="100">
                <comment>目录中文名称</comment>
            </column>
        </property>
        <property name="code" type="java.lang.String">
            <column name="code" length="200">
                <comment>本目录代码</comment>
            </column>
        </property>
        <set name="documentAuthoritys" inverse="true" cascade="save-update">
			<key column="did"/>
			<one-to-many class="DocumentAuthority"/>
    	</set>
    </class>
</hibernate-mapping>
