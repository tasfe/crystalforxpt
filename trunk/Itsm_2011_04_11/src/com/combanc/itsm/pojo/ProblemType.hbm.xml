<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.combanc.itsm.pojo">
    <class name="ProblemType" table="problem_type" catalog="itsm">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native"></generator>
        </id>
        <property name="problemCate" type="java.lang.String">
            <column name="problem_cate" length="100">
                <comment>类别英文名称</comment>
            </column>
        </property>
        <property name="problemCateSC" type="java.lang.String">
            <column name="problem_cate_SC" length="100">
                <comment>类别中文名称</comment>
            </column>
        </property>
        <property name="code" type="java.lang.String">
            <column name="code" length="200">
                <comment>目录结构代码</comment>
            </column>
        </property>
        <property name="pid" type="java.lang.Integer">
            <column name="pid">
                <comment>类别代码</comment>
            </column>
        </property>
        <property name="addTable1" type="java.lang.Integer">
            <column name="add_table1">
                <comment>增补描述表单</comment>
            </column>
        </property>
        <property name="addTable2" type="java.lang.Integer">
            <column name="add_table2">
                <comment>增补解决方案表单</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
