<public:attach event="onmouseover" onevent="divOnMouseOver()" />
<public:attach event="onmouseout" onevent="divOnMouseOut()" />

<script language="JavaScript">
function divOnMouseOver() {
	if(this.innerText != "" && this.innerText != "-")
	{
		var cs = this.currentStyle.cursor;
		this.className = "DivOver";
		this.style.cursor = cs;
	}
}
function divOnMouseOut() {
	this.className = "DivOut";
}
</SCRIPT>