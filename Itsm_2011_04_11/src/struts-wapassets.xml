<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="wapassets" extends="struts-default" namespace="/wapassets">
		<global-results>
			<result name="search" type="redirectAction">search</result>
		</global-results>
		<action name="search" class="WapAssetsAction" method="search">
			<result name="success" type="dispatcher">
				/wap/assets/searchwap.jsp
			</result>
		</action>
		<action name="main" class="WapAssetsAction" method="main">
			<result name="success" type="dispatcher">
				/wap/assets/searchwap.jsp
			</result>
		</action>
		<action name="change" class="WapAssetsAction" method="change">
			<result name="success" type="dispatcher">
				/wap/assets/changewap.jsp
			</result>
		</action>
		<action name="changesave" class="WapAssetsAction" method="changesave">
			<result name="success" type="dispatcher">
				/wap/assets/changewap.jsp
			</result>
		</action>
		<action name="functionlist" class="WapAssetsAction" method="functionlist">
			<result name="success" type="dispatcher">
				/wap/assets/functionlist.jsp
			</result>
		</action>	
		<action name="inStorechoose" class="WapAssetsAction" method="inStorechoose">
			<result name="success" type="dispatcher">
				/wap/assets/inStorechoose.jsp
			</result>
		</action>
		<action name="instoresave" class="WapAssetsAction" method="instoresave">
			<result name="success" type="dispatcher">
				/wap/assets/inStorechoose.jsp
			</result>
		</action>						
	</package>
</struts>
