<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.combanc.itsm.pojo">
	<class name="DocumentAuthority"
		table="document_authority" catalog="itsm">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native"></generator>
		</id>
		<property name="did" type="java.lang.Integer">
			<column name="did">
				<comment>文档库目录id</comment>
			</column>
		</property>
		<property name="tid" type="java.lang.Integer">
			<column name="tid">
				<comment>工程师分组id</comment>
			</column>
		</property>
		<property name="authoritySel" type="java.lang.Integer">
			<column name="authority_sel" length="1">
				<comment>查看权限</comment>
			</column>
		</property>
		<property name="authorityAdd" type="java.lang.Integer">
			<column name="authority_add" length="1">
				<comment>添加权限</comment>
			</column>
		</property>
		<property name="authorityDel" type="java.lang.Integer">
			<column name="authority_del" length="1">
				<comment>删除权限</comment>
			</column>
		</property>
		<property name="authorityUpd" type="java.lang.Integer">
			<column name="authority_upd" length="1">
				<comment>修改权限</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
